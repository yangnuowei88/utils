{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "docs/",
    "encoding": "utf8",
    "verbose": true,
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light",
      "title": "GeoOnline 基础JS工具库",
      "menu": [
        {
          "title": "GitLab 仓库",
          "link": "http://git.geoonline.geo/geoonline_team1/V10/geoonlineutil_front/-/tree/develop",
          "target": "_blank"
        }
      ]
    }
  }
}
